{
  "name": "spajam-2022-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint --fix --ext .js,.ts,.tsx --ignore-path .gitignore",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,ts,tsx,json}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/react": "~18.0.0",
    "@types/react-native": "~0.69.1",
    "expo": "~46.0.8",
    "expo-status-bar": "~1.4.0",
    "react": "18.0.0",
    "react-native": "0.69.4",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@react-native-community/eslint-config": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "private": true,
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.{js,ts,tsx,json}": "prettier --write"
  }
}
